{
	"manifest_version": 2,
	"name": "Math Anywhere",
	"description": "Render LaTeX & MathML formulas on any page you want",
	"version": "1.1",
	"icons": {
		"48": "48.png",
		"128": "128.png"
	},
	"permissions": [
		"tabs", "<all_urls>"
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Enable Math on this domain"
	},
	"background": {
		"scripts": ["mathml-chrome.js"]
	},
	"options_page": "options.html"
}