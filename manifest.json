{
	"name": "MathML anywhere",
	"description": "Render LaTeX & MathML formulas on any page you want",
	"version": "1.0",
	"icons": {
		"48": "48.png",
		"128": "128.png"
	},
	"permissions": [
		"tabs", "http://*/*"
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "MathML",
		"default_popup": "popup.html"
	},
	"background": {
		"scripts": ["background.js"]
	}
}