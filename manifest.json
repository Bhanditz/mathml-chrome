{
	"name": "Math anywhere",
	"description": "Render LaTeX & MathML formulas on any page you want",
	"version": "1.0",
	"icons": {
		"48": "48.png",
		"128": "128.png"
	},
	"permissions": [
		"tabs", "http://*/*", "https://*/*"
	],
	"browser_action": {
		"default_icon": "icon.png",
		"default_title": "Enable Math on this domain"
	},
	"background_page": "background.html"
}